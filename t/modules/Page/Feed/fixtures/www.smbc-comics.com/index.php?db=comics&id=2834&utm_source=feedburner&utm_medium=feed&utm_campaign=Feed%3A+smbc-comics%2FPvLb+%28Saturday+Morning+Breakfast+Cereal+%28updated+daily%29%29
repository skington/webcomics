<script language="JavaScript" type="text/javascript">
  var agt=navigator.userAgent.toLowerCase();
  var is_safari = (agt.indexOf("safari")!=-1);
  var is_firefox = (agt.indexOf("firefox")!=-1);
  var is_aol = (agt.indexOf("america online")!=-1);
  var is_opera = (agt.indexOf("opera")!=-1);
  var is_macie = (agt.indexOf("msie")!=-1 && agt.indexOf("mac")!=-1);
  var is_camino = (agt.indexOf("camino")!=-1 && agt.indexOf("mac")!=-1);
  var is_winie = (agt.indexOf("msie")!=-1 && agt.indexOf("windows")!=-1);
  if (!is_macie && !is_aol && !is_opera && !is_camino) {
  }
  if (!is_firefox && !is_opera) {
    document.write("<\/td><\/tr><tr><td>");
  }
  function getBlock(name) {
    if (document.getElementById) {
      return document.getElementById(name);
    } else if (document.all) {
      return document.all[name];
    } else if (document.layers) {
      return document.layers[name];
    }
  }

  function jumpToRandom() {
    var num = Math.floor(Math.random()*2839)
    window.location = 'http://www.smbc-comics.com/?db=comics&id='+num+'#comic';
  }

  function getStyle(name) {
    return getBlock(name).style;
  }

  function hideBlock(name) {
    getStyle(name).display="none";
  }

  function showBlock(name) {
    getStyle(name).display="";
  }

  if (document.layers) {
    document.captureEvents(Event.MOUSEMOVE);
  }
  document.onmousemove = captureMousePosition;

  xMousePos = 0;
  yMousePos = 0;
  function captureMousePosition(e) {
    if (document.layers) {
      xMousePos = e.pageX;
      yMousePos = e.pageY;
    } else if (document.all) {
      xMousePos = window.event.x+document.body.scrollLeft;
      yMousePos = window.event.y+document.body.scrollTop;
    } else if (document.getElementById) {
      xMousePos = e.pageX;
      yMousePos = e.pageY;
    }
  }

  var mouseOverMenu=false;
  var mouseOverSubmenu=false;
  var currentMenu=null;
  function onMouseOverMenu(name) {
    mouseOverMenu = true;
    if (currentMenu == null) {
      currentMenu=name;
      setTimeout("showSubmenu('"+name+"')", 500);
    } else {
      currentMenu=name;
      hideSubmenu(name);
      showSubmenu(name);
    }
  }
  function onMouseOverSubmenu() {
    mouseOverSubmenu = true;
    showBlock(currentMenu);
  }
  function showSubmenu(name) {
    if (mouseOverSubmenu || mouseOverMenu) {
      if(getStyle(currentMenu).display=='none') {
        var tx = xMousePos-20;
        var ty = yMousePos-180;
        tx = tx<0?0:tx;
        ty = ty<0?0:ty;
        getStyle(currentMenu).left = tx+"px";
        getStyle(currentMenu).top  = ty+"px";
      }
      showBlock(currentMenu);
    }
  }
  function hideSubmenu(name) {
    if (!mouseOverSubmenu && !mouseOverMenu) {
      hideBlock("aftercomic");
      currentMenu=null;
    } else {
      if (currentMenu != "aftercomic") { hideBlock("menu1"); }
    }
  }
  function onMouseOutMenu(name) {
    mouseOverMenu = false;
    setTimeout("hideSubmenu('"+name+"')", 250);
  }
  function onMouseOutSubmenu(name) {
    mouseOverSubmenu = false;
    setTimeout("hideSubmenu('"+name+"')", 250);
  }

// a - 65
// b - 66
// up - 38
// left - 37
// right - 39
// down - 40
var konamiSeq = 0;
var konamiArray = Array(38,38,40,40,37,39,37,39,66,65);
// Poor mans code checker
function checkCodes(keycode) {
  if (konamiArray[konamiSeq]==keycode) {
    konamiSeq++;
  } else {
    konamiSeq = 0;
  }
  if (konamiSeq == konamiArray.length) {
    showBlock("konamicomic");
  }
}

function onKeyUp(evt) {
  if (evt = evt ? evt : window.event ? event : null) {
    checkCodes(evt.keyCode);
    switch (evt.keyCode) {
      case 90: // z
      case 188: // <
      case 89: // y
        document.location.href = "/index.php?db=comics&id=2833#comic"; break;
      case 88: // x
      case 191: // ?
        jumpToRandom(); break;
      case 190: // >
      case 67: // c
        document.location.href = "/index.php?db=comics&id=2835#comic"; break;
    }
  }
}
if (document.addEventListener) {
    document.addEventListener("keyup", onKeyUp, false);
} else {
    document.onkeyup = onKeyUp;
}
</script>
