<script type="text/javascript">

    $(function(){
				
		$('#enhanced_comic_nav_menu').menu({
			content: $('#enhanced_comic_nav_menu').next().html(),
			backLink: true,
			maxHeight: 350,
			width: 350,
			crumbDefaultText: 'Choose a story',
            flyOut: false,
            linkToFront: true
		});
    });
	
	function pad(number, length){
		var str = "" + number;
		while(str.length<length){
			str = '0'+str;
	   	}
	   	return str;
	}


	function navigate(url) { 
		window.location.href = url;
	}

	$(document).ready(function() {
		$("#comic_nav_dropdown").change( function() {
			comic_nav_value = $("#comic_nav_dropdown").val();
       		window.location.href= "/comics/archives/daily/" + comic_nav_value;
		});
	});
	
	var checkStruct = {};
	var lock = {};

	function dateToShortFmt(dt) {
		return (dt.getMonth() + "/" + dt.getDate() + "/" + dt.getFullYear());
	}
		
	function dateToIdFmt(dt) {
		return (dt.getFullYear()+""+pad(dt.getMonth()+1, 2)+""+pad(dt.getDate(), 2));
	}
	
	function dateToLocaleString(dt, cal) {
		var wStr = cal.cfg.getProperty("WEEKDAYS_LONG")[dt.getDay()];
		var dStr = dt.getDate();
		var mStr = cal.cfg.getProperty("MONTHS_LONG")[dt.getMonth()];
		var yStr = dt.getFullYear();
		return (wStr + ", " + dStr + " " + mStr + " " + yStr);
	}

	function calSelectHandler(dateStr) {
		window.location.href = "/comics/archives/daily/" + dateStr;
	};

    function handleData(month, year, data) {
        key = month + '/' + year;
        checkStruct[key] = data;
        lock[key] = false;
    }

	var newMonth = function(year, month, cal) {			
	   key = month + '/' + year;
		if (!checkStruct[key] && !lock[key]) {
			lock[key] = true;
			valid =  $.getJSON("/comics/navigation/calendar_date_check?month=" + month + "&year=" + year,
				function (data) {
					month = data.month;
					year = data.year;
					data = data.data;
                    handleData(month, year, data);
				});
		}
		else if (lock[key]) {
			//console.debug("Lock set for month #" + month);
		}
		else {
			//console.debug("Already have data for month #" + month);
		}
    }  
    
    function pause(numberMillis)
    {
        var now = new Date();
        var exitTime = now.getTime() + numberMillis;
        while (true)
        {
            now = new Date();
            if (now.getTime() > exitTime)
                return;
        }
    }
    
    var checkDay = function(date) {
        var day = date.getDate();
        var month = date.getMonth() + 1;
        var year = date.getFullYear();
        key = month + '/' + year;
        struct = checkStruct[key];
        valid = struct[day];
        return [valid, '', ''];
    }
    
	$(function() {
		$("#comicNavCalContainer").datepicker({ 
            showButtonPanel: false,
            showOtherMonths: true,
            dateFormat: 'yymmdd', 
            minDate: new Date(Date.parse("08/25/1997")),  
            maxDate: new Date(Date.parse("12/27/2012")), 
            defaultDate: new Date(Date.parse("12/27/2012")),
            onSelect: calSelectHandler,
            onChangeMonthYear: newMonth
        });
	});


</script>


 
	


