				<li class="NAV_RSS"><a href="/rss/" title="RSS"><span>RSS</span></a></li>
				<li class="NAV_Mobile"><a href="/mobile/" title="Mobile"><span>Mobile</span></a></li>
				<li class="NAV_SignUp"><a href="/register/" title="Sign-Up"><span>Sign-Up</span></a></li>
			</ul>
		</div>
		<script language="javascript">
			$(function(){
				$('.FRM_TextInput').focus(function() {
					if ($(this).hasClass('FRM_Password')) {
						if ($(this).attr('name') == 'PasswordDisplay') {
							$(this).css('display','none');
							$('#HDR_Password').css('display','block').focus();
						}
					} else {
						$(this).val(($(this).val() == $(this).attr('name')) ? '' : $(this).val());

						if($(this).val() == $(this).attr('name'))
							$(this).removeClass('NormalStyle');
						else
							$(this).addClass('NormalStyle');
					}
				});

				$('.FRM_TextInput').blur(function() {
					if ($(this).hasClass('FRM_Password')) {
						if ($(this).attr('name') == 'Password' && $(this).val() == '') {
							$(this).css('display','none');
							$('#HDR_PasswordDisplay').css('display','block');
						}
					} else {
						$(this).val(($(this).val() == '') ? $(this).attr('name') : $(this).val());
						if($(this).val() != $(this).attr('name'))
							$(this).addClass('NormalStyle');
						else
							$(this).removeClass('NormalStyle');
					}
				});
			});
		</script>
			</div>

